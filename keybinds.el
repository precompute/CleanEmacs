(global-set-key [f9] 'save-buffer)

(global-set-key (kbd "C-f") 'hippie-expand)

(global-set-key (kbd "s-h")     'evil-backward-char)
(global-set-key (kbd "s-l")     'evil-forward-char)
(global-set-key (kbd "s-j")     'evil-next-line)
(global-set-key (kbd "s-k")     'evil-previous-line)
